Private Sub Picture9_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    Call IconSet
    If Parent.WindowState = 2 Or WindowR = 1 Then: Exit Sub
    If Button = 1 Then
        If Parent.Height - Y > 480 Then
            Parent.Top = Parent.Top + Y
            Parent.Height = Parent.Height - Y
            Call Resize
        End If
    End If
End Sub

Private Sub UserControl_Resize()
    Call Resize
End Sub

Private Sub Resize()
On Error Resume Next
    Picture11.Visible = False
    UserControl.Width = Parent.Width
    UserControl.Height = Parent.Height

    Picture1.Width = UserControl.Width
    Picture2.Width = UserControl.Width
    Picture3.Height = UserControl.Height
    Picture4.Height = UserControl.Height
    Picture9.Width = UserControl.Width
    
    Picture2.Top = UserControl.Height - Picture2.Height
    Picture4.Left = UserControl.Width - Picture4.Width
    
    Picture5.Left = 0
    Picture5.Top = 0
    Picture6.Left = UserControl.Width - Picture6.Width
    Picture6.Top = 0
    Picture7.Left = 0
    Picture7.Top = UserControl.Height - Picture7.Height
    Picture8.Left = UserControl.Width - Picture8.Width
    Picture8.Top = UserControl.Height - Picture8.Height
    Picture9.Left = 0
    Picture9.Top = 0
    
    Picture10.Left = UserControl.Width - Picture4.Width - Picture10.Width
    Picture10.Top = 0
    Picture11.Left = Picture10.Left - Picture11.Width
    Picture11.Top = 0
    Picture12.Left = Picture10.Left - Picture12.Width
    Picture11.Top = 0
    
    Label1.Width = UserControl.Width - 1815
    Image1.Picture = Parent.Icon
    Label1.Caption = ""
End Sub
