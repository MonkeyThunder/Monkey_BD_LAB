Private Declare Function SendMessage Lib "user32" Alias "SendMessageA" _
(ByVal hWnd As Long, ByVal wMsg As Long, ByVal wParam As Long, lParam As Any) As Long
Private Declare Sub ReleaseCapture Lib "user32" ()

Const WM_NCLBUTTONDOWN = &HA1
Const HTCAPTION = 2

Dim WindowR As Long

Private Sub Label1_DblClick()
    Call Picture11_Click
End Sub

Private Sub Label1_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    Call IconSet
    
    If Parent.WindowState = 2 Then: Exit Sub
    Dim lngReturnValue As Long
    If Button = 1 Then
        Call ReleaseCapture
        lngReturnValue = SendMessage(Parent.hWnd, WM_NCLBUTTONDOWN, HTCAPTION, 0&)
    End If
End Sub

Private Sub Picture1_DblClick()
    Call Picture11_Click
End Sub

Private Sub Picture1_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    Call IconSet
    
    If Parent.WindowState = 2 Then: Exit Sub
    Dim lngReturnValue As Long
    If Button = 1 Then
        Call ReleaseCapture
        lngReturnValue = SendMessage(Parent.hWnd, WM_NCLBUTTONDOWN, HTCAPTION, 0&)
    End If
End Sub

Private Sub IconSet()
    Picture10.Picture = Picture13(0)
    Picture11.Picture = Picture13(1)
    Picture12.Picture = Picture13(2)
End Sub

Private Sub Picture10_Click()
    Unload Parent
End Sub
Private Sub Picture11_Click()
    If WindowR = 1 Then: Exit Sub
    If Parent.WindowState = 0 Then
        Parent.WindowState = 2
    Else
        Parent.WindowState = 0
    End If
    
    Call Full
    Call Resize
End Sub

Private Sub Full()
    If Parent.WindowState = 2 Then
        Picture2.MousePointer = 0
        Picture3.MousePointer = 0
        Picture4.MousePointer = 0
        Picture5.MousePointer = 0
        Picture6.MousePointer = 0
        Picture7.MousePointer = 0
        Picture8.MousePointer = 0
        Picture9.MousePointer = 0
    Else
        Picture2.MousePointer = 7
        Picture3.MousePointer = 9
        Picture4.MousePointer = 9
        Picture5.MousePointer = 8
        Picture6.MousePointer = 6
        Picture7.MousePointer = 6
        Picture8.MousePointer = 8
        Picture9.MousePointer = 7
    End If
End Sub

Public Sub DisableSizeChange(ByVal SizeChange As Long)
    WindowR = SizeChange
    Call WindowRL
End Sub

Private Sub WindowRL()
    If WindowR = 1 Then
        Picture2.MousePointer = 0
        Picture3.MousePointer = 0
        Picture4.MousePointer = 0
        Picture5.MousePointer = 0
        Picture6.MousePointer = 0
        Picture7.MousePointer = 0
        Picture8.MousePointer = 0
        Picture9.MousePointer = 0
        
        Picture11.Visible = False
    Else
        Picture2.MousePointer = 7
        Picture3.MousePointer = 9
        Picture4.MousePointer = 9
        Picture5.MousePointer = 8
        Picture6.MousePointer = 6
        Picture7.MousePointer = 6
        Picture8.MousePointer = 8
        Picture9.MousePointer = 7
        
        Picture11.Visible = True
    End If
End Sub
Private Sub Picture12_Click()
    Parent.WindowState = 1
End Sub

Private Sub Picture10_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
Call IconSet
    Picture10.Picture = Picture13(3)
End Sub
Private Sub Picture11_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
Call IconSet
    Picture11.Picture = Picture13(4)
End Sub
Private Sub Picture12_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
Call IconSet
    Picture12.Picture = Picture13(5)
End Sub

Private Sub Picture2_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    If Parent.WindowState = 2 Or WindowR = 1 Then: Exit Sub
    If Button = 1 Then
        If Parent.Height + Y > 480 Then
            Parent.Height = Parent.Height + Y
            Call Resize
        End If
    End If
End Sub

Private Sub Picture3_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    If Parent.WindowState = 2 Or WindowR = 1 Then: Exit Sub
    If Button = 1 Then
        If Parent.Width - X > 1700 Then
            Parent.Left = Parent.Left + X
            Parent.Width = Parent.Width - X
            Call Resize
        End If
    End If
End Sub

Private Sub Picture4_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    Call IconSet
    If Parent.WindowState = 2 Or WindowR = 1 Then: Exit Sub
    If Button = 1 Then
        If Parent.Width + X > 1700 Then
            Parent.Width = Parent.Width + X
            Call Resize
        End If
    End If
End Sub

Private Sub Picture5_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    If Parent.WindowState = 2 Or WindowR = 1 Then: Exit Sub
    If Button = 1 Then
        If Parent.Width - X > 1700 Then
            Parent.Left = Parent.Left + X
            Parent.Width = Parent.Width - X
        End If
        If Parent.Height - Y > 480 Then
            Parent.Top = Parent.Top + Y
            Parent.Height = Parent.Height - Y
        End If
        Call Resize
    End If
End Sub

